{
    "device": "cuda",
    "dataset": {
        "training": "Temp2D/Train/",
        "testing": "Temp2D/Test/",
        "unittest": "Temp2D/Test/",
        "mode":"2D"
    },
    "visualization": {"volume": "volume-0.nii", "mask": "segmentation-0.nii"},
    "unittest_size": [16, 16, 8],
    "unittest_batch": 1,
    "network_name": "monai_2DUNet",
    "network_parameters": {
        "in_channels": 1,
        "out_channels": 1,
        "spatial_dims": 2,
        "channels": [64, 128, 256, 512],
        "strides": [2, 2, 2],
        "num_res_units": 4,
        "bias": 1,
        "norm": "INSTANCE",
        "dropout": 0.4
    },
    "training": {
        "epochs": 100,
        "batch_size": 12,
        "train_valid_split": 0,
        "optimizer": "Adam",
        "optimizer_parameters": {"lr": 0.01},
        "lr_scheduler": "StepLR",
        "scheduler_parameters": {"step_size": 20, "gamma": 0.5, "verbose": 1},
        "loss_name": "monai_dice",
        "loss_parameters": {"sigmoid": "1", "batch": 1, "include_background": 1},
        "metrics": "dice",
        "metrics_parameters": {"ignore_empty": 1, "include_background": 1},
        "shuffle": 1
    },
    "transforms": {
        "transformation_size": [512, 512],
        "img_key": "image",
        "label_key": "label",
        "pred_key": "pred",
        "sw_batch_size": 2,
        "roi_size": [192, 192, 32],
        "overlap": 0.25,
        "train_transform": "3d_ct_transform",
        "test_transform": "3d_ct_transform",
        "post_transform": "3d_ct_transform"
    },
    "save": {
        "volume_png_path": "Temp2D/volume/",
        "mask_png_path": "Temp2D/mask/",
        "model_checkpoint": "model_checkpoint",
        "potential_checkpoint": "potential_checkpoint",
        "tensorboard": "runs/"
    },
    "test": {
        "train_folder": "tests/testdata/resized_train_liver/",
        "test_folder": "tests/testdata/resized_liver/",
        "test_volume": "tests/testdata/resized_liver/volume/resized_liver.nii",
        "test_label": "tests/testdata/resized_liver/mask/resized_mask.nii",
        "reference_sliding_window": "tests/testdata/reference_liver_sw",
        "liver_cp": "tests/testdata/liver_cp",
        "lesion_cp": "tests/testdata/lesion_cp",
        "reference_lesion_cp": "tests/testdata/reference_lesion_cp"

    },
    "seed": 0
}
