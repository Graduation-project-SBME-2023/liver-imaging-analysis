{
    "device": "cpu",
    "dataset": {
        "mode":"2D",
        "training": "Temp2D/Train/",
        "testing": "Temp2D/Test/",
        "unittest": "Temp2D/Test/"

    },
    "visualization":{
        "volume":"Liver-Segmentation-Website/static/img/volume.nii",
        "mask":"Liver-Segmentation-Website/static/img/mask.nii"
    },
    "unittest_size": [16, 16, 8],
    "unittest_batch": 1,
    "network_name": "monai_2DUNet",
    "network_parameters": {
        "in_channels": 1, 
        "out_channels": 1,
        "spatial_dims":2, 
        "channels": [64, 128, 256, 512],
        "strides": [2, 2, 2],
        "num_res_units":4, 
        "bias":1,
        "norm":"INSTANCE",
        "dropout":0.4
    },
    "training": {
        "epochs": 100,
        "batch_size": 12,
        "train_valid_split": 0,
        "optimizer": "Adam",
        "optimizer_parameters": {"lr": 0.01},
        "lr_scheduler":"StepLR",
        "scheduler_parameters":{"step_size":20, "gamma":0.5, "verbose":1},
        "loss_name": "monai_dice",
        "loss_parameters": {"sigmoid":"1","batch":1,"include_background":1},
        "metrics":"dice",
        "metrics_parameters":{"ignore_empty":1,"include_background":1},
        "shuffle":1
    },
    "transforms": {
        "transformation_size": [512, 512],
        "img_key": "image",
        "label_key": "label",
        "pred_key": "pred",
        "sw_batch_size": 2,
        "roi_size": [192, 192, 32],
        "overlap": 0.25,
        "train_transform": "3DUnet_transform",
        "test_transform": "3DUnet_transform",
        "post_transform": "3DUnet_transform"
    },
    "save": {
        "volume_png_path": "Temp2D/volume/",
        "mask_png_path": "Temp2D/mask/",
        "model_checkpoint": "model_checkpoint",
        "potential_checkpoint": "potential_checkpoint",
        "tensorboard": "runs/"
    },
    "seed": 0
}
