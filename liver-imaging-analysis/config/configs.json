{
    "device": "cuda",
    "dataset": {
        "mode":"2D",
        "training": "/content/Temp2D/Train/",
        "testing": "/content/Temp2D/Test/",
        "unittest": "/content/Temp2D/Test/"
    },
    "unittest_size": [16, 16, 8],
    "unittest_batch": 1,
    "network_name": "monai_2DUNet",
    "network_parameters": {
        "in_channels": 1, 
        "out_channels": 1,
        "spatial_dims":2, 
        "channels": [16, 32, 64, 128, 256],
        "strides": [2, 2, 2, 2]
    },
    "training": {
        "epochs": 1,
        "batch_size": 8,
        "train_valid_split": 0,
        "optimizer": "Adam",
        "optimizer_parameters": {"lr": 0.01},
        "lr_scheduler":"StepLR",
        "scheduler_parameters":{"step_size":3, "gamma":0.5},
        "loss_name": "monai_dice",
        "loss_parameters": {"sigmoid":"1"},
        "metrics":"dice",
        "metrics_parameters":{},
        "shuffle":1
    },
    "transforms": {
        "transformation_size": [256, 256],
        "img_key": "image",
        "label_key": "label",
        "train_transform": "2DUnet_transform",
        "test_transform": "2DUnet_transform"
    },
    "save": {
        "volume_png_path": "/content/Temp2D/volume/",
        "mask_png_path": "/content/Temp2D/mask/",
        "model_checkpoint": "/content/drive/MyDrive/liver-imaging-analysis/engine/model_checkpoint",
        "potential_checkpoint": "/content/drive/MyDrive/liver-imaging-analysis/engine/potential_checkpoint",
        "tensorboard": "/content/drive/MyDrive/liver-imaging-analysis/engine/runs/"
    },
    "seed": 0
}
